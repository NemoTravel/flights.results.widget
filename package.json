	"license": "MIT",
	"bugs": {
		"url": "https://github.com/NemoTravel/results/issues"
	},
	"homepage": "https://github.com/NemoTravel/results#readme",
	"devDependencies": {
		"@types/webpack": "^4.4.1",
		"autoprefixer": "^7.2.6",
		"compression": "^1.7.2",
		"cross-env": "^5.2.0",
		"css-loader": "^0.28.11",
		"express": "^4.16.3",
		"jest": "^22.4.4",
		"mini-css-extract-plugin": "^0.4.0",
