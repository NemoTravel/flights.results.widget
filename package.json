	"jest": {
		"modulePaths": [
			"/src"
		],
		"moduleFileExtensions": [
			"js",
			"ts",
			"jsx",
			"tsx"
		],
		"moduleDirectories": [
			"node_modules",
			"src"
		],
		"transform": {
			"^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
		},
		"testMatch": [
			"**/__tests__/*.+(ts|tsx|js)"
		],
		"moduleNameMapper": {}
	}
}
