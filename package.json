		"tslint": "^5.10.0",
		"typescript": "^2.9.2",
		"url-loader": "^0.5.9",
		"webpack": "^4.12.0",
		"webpack-cli": "^2.1.5",
		"why-did-you-update": "^0.1.1"
	},
	"dependencies": {
		"@material-ui/core": "^1.2.3",
