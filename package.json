{
	"name": "@nemo.travel/results-widget",
	"version": "0.0.1",
	"description": "Flexible flights search results",
	"main": "dist/flights.results.widget.min.js",
	"scripts": {
		"tslint": "tslint -p ./tslint.json",
		"server": "node server.js",
		"build": "npm run tslint && npm test && cross-env NODE_ENV=production webpack",
		"build-dev": "cross-env NODE_ENV=dev webpack",
		"dev": "run-p build-dev server",
		"prepare": "npm run build",
		"test": "jest",
		"start": "npm run server"
	},
	"pre-commit": [
		"tslint",
		"test"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/NemoTravel/results.git"
	},
	"keywords": [],
	"author": "Paul Naumov <p.naumov@nemo.travel>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/NemoTravel/results/issues"
	},
	"homepage": "https://github.com/NemoTravel/results#readme",
	"devDependencies": {
		"@types/classnames": "^2.2.3",
		"@types/js-cookie": "2.1.0",
		"@types/jest": "^22.2.0",
		"@types/moment": "^2.13.0",
		"@types/moment-duration-format": "^2.2.0",
		"@types/promise-polyfill": "^6.0.0",
		"@types/react": "^16.0.38",
		"@types/react-dom": "^16.0.4",
		"@types/react-redux": "^5.0.15",
		"@types/react-swipeable-views": "^0.12.1",
		"@types/redux-logger": "^3.0.5",
		"@types/redux-thunk": "^2.1.32",
		"@types/reselect": "^2.2.0",
		"autoprefixer": "^7.2.6",
		"compression": "^1.7.2",
		"cross-env": "^5.0.5",
		"css-loader": "^0.28.7",
		"express": "^4.15.4",
		"extract-text-webpack-plugin": "^3.0.0",
		"jest": "^22.4.2",
		"node-sass": "^4.6.1",
		"npm-run-all": "^4.1.2",
		"opn": "^5.1.0",
		"postcss-loader": "^2.0.6",
		"pre-commit": "^1.2.2",
		"redux-testkit": "^1.0.6",
		"resolve-url-loader": "^2.1.0",
		"sass-loader": "^6.0.6",
		"style-loader": "^0.18.2",
		"ts-loader": "^3.5.0",
