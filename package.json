		"@types/react-virtualized": "^9.7.12",
		"classnames": "^2.2.5",
		"material-ui": "^1.0.0-beta.34",
		"material-ui-icons": "^1.0.0-beta.17",
		"moment": "^2.19.2",
		"moment-duration-format": "^2.2.2",
		"promise-polyfill": "^6.0.2",
		"react": "^15.6.1",
		"react-dom": "^15.6.1",
		"react-redux": "^5.0.7",
		"react-swipeable-views": "^0.12.12",
		"react-virtualized": "^9.18.5",
		"redux": "^3.7.2",
